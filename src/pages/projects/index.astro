---
import BaseHead from '../../components/BaseHead.astro';
import Header from '../../components/Header.astro';
import Footer from '../../components/Footer.astro';
import { SITE_TITLE, SITE_DESCRIPTION } from '../../consts';
---

<!doctype html>
<html lang="en">
  <head>
    <BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
  </head>
  <body>
    <Header title={SITE_TITLE} />

    <main class="p-8">
      <ul>
        <li>
          <a
            href="https://github.com/zalnaRs/CarLinkShortcut"
            target="_blank"
            class="underline hover:no-underline hover:font-semibold"
            >CarLinkShortcut</a
          >
        </li>
      </ul>
    </main>
    <li class="mt-32 m-8 list-none">
      <a
        href="/projects/general-privacy"
        target="_blank"
        class="underline hover:no-underline hover:font-semibold"
        >General Privacy Policy</a
      >
    </li>

    <Footer />
  </body>
</html>
